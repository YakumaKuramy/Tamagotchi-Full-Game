;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Tamagotchi Full Game"
run/main_scene="res://scenes/system/system/main_game.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

GeralBgm="*res://scenes/resource/gerals/geral_bgm.tscn"
GeralSfx="*res://scenes/resource/gerals/geral_sfx.tscn"

[display]

window/size/viewport_width=750
window/size/viewport_height=680
window/stretch/mode="canvas_items"

[editor_plugins]

enabled=PackedStringArray()

[global_group]

buttons=""

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
